@font-face {
	font-family: 'SV-Icons';
	src:url(font-path("SV-Icons.eot"));
	src:url(font-path('SV-Icons.eot?#iefix')) format('embedded-opentype'),
		url(font-path('SV-Icons.woff')) format('woff'),
		url(font-path('SV-Icons.ttf')) format('truetype'),
		url(font-path('SV-Icons.svg#SV-Icons')) format('svg');
	font-weight: normal;
	font-style: normal;
}

/* Use the following CSS code if you want to use data attributes for inserting your icons */
[data-icon]:before {
	font-family: 'SV-Icons';
	content: attr(data-icon);
	speak: none;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
}

/* Use the following CSS code if you want to have a class per icon */
/*
Instead of a list of all class selectors,
you can use the generic selector below, but it's slower:
[class*="icon-"]:before {
*/
.icon-play-alt, .icon-arrow-right, .icon-uniF473, .icon-arrow, .icon-uniF48A, .icon-uniF48B, .icon-list, .icon-bullet-list, .icon-calendar, .icon-search {
	font-family: 'SV-Icons';
	speak: none;
  font-size:0.8em;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
  margin-left:0.3em;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
}
.icon-play-alt:before {
	content: "\e001";
}
.icon-arrow-right:before {
	content: "\e002";
}
.icon-uniF473:before {
	content: "\f473";
}
.icon-arrow:before {
	content: "\e000";
}
.icon-uniF48A:before {
	content: "\f48a";
}
.icon-uniF48B:before {
	content: "\f48b";
}
.icon-list:before {
	content: "\e003";
}
.icon-bullet-list:before {
	content: "\e004";
}
.icon-calendar:before {
	content: "\e005";
}
.icon-search:before {
	content: "\e006";
}
